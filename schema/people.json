{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"nameSlug": {
			"type": "string",
			"pattern": "^[a-z0-9]+[a-z0-9-]*$"
		},
		"nationality": {
			"type": "string"
		},
		"dateOfBirth": {
			"type": "string",
			"pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
		}
	},
	"required": [
		"name",
		"nameSlug",
		"nationality",
		"dateOfBirth"
	]
}
