{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"required": [
		"name",
		"foundedIn",
		"foundedAt",
		"website"
	],
	"additionalProperties": false,
	
	"properties": {
		"name": {
			"type": "string"
		},
		"foundedIn": {
			"type": "string"
		},
		"foundedAt": {
			"type": "integer"
		},
		"website": {
			"type": "string"
		}
	}
}
