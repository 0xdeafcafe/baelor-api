{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"required": [
		"name",
		"founded_at",
		"founded_in",
		"location",
		"website"
	],
	"additionalProperties": false,

	"properties": {
		"name": {
			"type": "string"
		},
		"founded_at": {
			"type": "integer"
		},
		"founded_in": {
			"type": "string"
		},
		"location": {
			"type": "string"
		},
		"website": {
			"type": "string"
		}
	}
}
