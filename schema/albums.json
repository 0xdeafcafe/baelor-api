{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"title": {
			"type": "string"
		},
		"titleSlug": {
			"type": "string",
			"pattern": "^[a-z0-9]+[a-z0-9-]*$"
		},
		"length": {
			"type": "integer",
			"minimum": 1
		}
	},
	"required": [
		"title",
		"titleSlug",
		"length"
	]
}
